@model IEnumerable<KaraokeModel>

@{
    ViewData["Title"] = "Index";
}
@if (Model.Count() > 0)
{
    <table class="table table-bordered table-striped" style="width:100%">
        <thead>
            <tr>
                <th>
                    Artist's Beginning with @ViewBag.FirstLetter
                </th>
                <th>
                    Display Order
                </th>
            </tr>
        </thead> 
        <tbody>
            @foreach (var obj in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => obj.Artist)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => obj.Title)
                    </td>
                    <td>
                        |
                        <a asp-action="Details" asp-route-id="@obj.Id">Details</a> |
                    </td>
                </tr>
            }

        </tbody>

    </table>
}